/dts-v1/;
#include "fsl-s32r45.dtsi"
#include <dt-bindings/pinctrl/s32r45-pinctrl.h>

/ {
	aliases {
		gmac0 = &gmac0;
		gmac1 = &gmac1;
	};

	chosen {
		linux,initrd-start = <0x84000000>;
		linux,initrd-end = <0x8A000000>;
		stdout-path = "serial0:115200n8";
	};

	memory_DDR0@80000000 {
		device_type = "memory";
		reg = <0 0x80000000 0 0x40000000>;
	};

	memory_DDR1@c0000000 {
		device_type = "memory";
		reg = <0 0xc0000000 0 0x40000000>;
	};

	memory_DDR2@880000000 {
		device_type = "memory";
		reg = <0x8 0x80000000 0 0x80000000>;
	};
};

&fxosc {
	clock-frequency = <40000000>;
};

