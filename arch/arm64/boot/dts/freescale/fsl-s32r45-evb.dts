&spi0 {
	status = "okay";
};

&spi1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl0_dspi1>, <&pinctrl1_dspi1>;
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";

	spidev10: spidev@0 {
		compatible = "rohm,dh2228fv";
		spi-max-frequency = <10000000>;
		reg = <0>;
	};

	spidev11: spidev@1 {
		compatible = "rohm,dh2228fv";
		spi-max-frequency = <10000000>;
		reg = <1>;
	};
};

&spi2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl0_dspi2>, <&pinctrl1_dspi2>;
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";

	spidev20: spidev@0 {
		compatible = "rohm,dh2228fv";
		spi-max-frequency = <10000000>;
		reg = <0>;
	};

	spidev21: spidev@1 {
		compatible = "rohm,dh2228fv";
		spi-max-frequency = <10000000>;
		reg = <1>;
	};
};

&spi3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl0_dspi3>, <&pinctrl1_dspi3>;
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";

	spidev3: spidev@0 {
		compatible = "rohm,dh2228fv";
		spi-max-frequency = <4000000>;
		reg = <0>;
	};
};

&spi5 {
	pinctrl-names = "default";
	pinctrl-0 =  <&pinctrl1_dspi5>;
	status = "okay";
};

&usdhc0 {
	status = "okay";
};

&qspi {
	status = "okay";

	mx25uw51245g@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		spi-max-frequency = <133000000>;
		spi-tx-bus-width = <8>;
		spi-rx-bus-width = <8>;
		reg = <0>;
	};
};

&gmac0 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl0_rgmii2>, <&pinctrl0_mdio2>;
	phy-mode = "rgmii";
	phy-handle = <&gmac0_phy1>;
	status = "okay";
};

&gmac0_mdio {
	/* KSZ9031RNXCA on S32R-VNP-PROC */
	gmac0_phy1: ethernet-phy@1 {
		reg = <1>;
	};
};

&gmac1 {
	phy-mode = "sgmii";
	status = "disabled";

	/* Current GMAC driver supports only CL22 but EVB phy
	 * supports only CL45, so no MDIO communication is
	 * possible.
	 */
	fixed-link {
		speed = <1000>;
		full-duplex;
	};
};
&gpio1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_gpioeirq>;
	status = "okay";
};
